{
  "name": "music-bracket-maker-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build && node dist/server/index.js",
    "build": "tsc",
    "test": "jest",
    "reset-db": "yarn run sequelize db:drop && yarn run sequelize db:create && yarn run sequelize db:migrate",
    "heroku-postbuild": "npm run build && cd client && npm install --no-shrinkwrap && npm run build",
    "dev": "concurrently \"yarn start\" \"cd client && yarn start\""
  },
  "dependencies": {
    "@types/react-router-dom": "^4.3.3",
    "apollo-server-express": "^2.6.2",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "graphql": "^14.3.1",
    "lodash": "^4.17.13",
    "moment": "^2.29.2",
    "mysql2": "^1.6.5",
    "pluralize": "^7.0.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^5.4.0",
    "typescript": "^3.5.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/jest": "^24.0.13",
    "babel-jest": "^24.8.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2"
  },
  "jest": {}
}
